<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="myorg" class="com.jamal.springDemo.domain.OrganisationCP"
		c:companyName="BMW" c:yearOfIncorporation="1929" c:address-ref="address" />

	<bean id="address" class="com.jamal.springDemo.domain.Address"
		p:street="Blue Berry Lane" p:city="Los Angeles" p:zipCode="75757"></bean>
</beans>
